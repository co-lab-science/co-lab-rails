
.general-info{
    flex: vertical;
    justify-content: start;
    width: 37%;
}
.specific-map{
    background-color: #ededed;
    width: 63%;
}
.general-overview {
    background-color: #1b79be;
    padding: 45px 15px 25px;
    background-color: #c6c6c6;
}
.general-overview h1, .general-overview h2 a, .general-overview h2, .general-overview h1 a, .general-overview h3, .general-overview h3 a{
    color: #1b79be;
    margin: 0;
    padding-bottom: 25px

}

.general-comments{
    padding: 15px;
    /*border-top: #1b79be solid 7px;*/
}

.general-info-title{
    background: url(<%= asset_data_uri 'mbri/mbri-left.svg' %>) no-repeat;
    background-position: 5px 12px;
    padding-left: 45px;
}
.general-info-create{
    background: url(<%= asset_data_uri 'mbri/mbri-edit.svg' %>) no-repeat;
    background-position: 10px 8px;
    padding-left: 50px;
}
.general-info-search{
    background: url(<%= asset_data_uri 'mbri/mbri-search.svg' %>) no-repeat;
    background-position: 9px 8px;
    padding-left: 50px;
}

.general-info-more{
    background: url(<%= asset_data_uri 'mbri/mbri-right.svg' %>) no-repeat;
    background-position: 5px 12px;
    padding-left: 45px;
}